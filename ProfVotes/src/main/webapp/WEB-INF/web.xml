<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ProfVotes</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<servlet-class>prof.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>SignupController</servlet-name>
  	<servlet-class>prof.controller.SignupController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignupController</servlet-name>
  	<url-pattern>/SignupController</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>adminLoginController</servlet-name>
  	<servlet-class>prof.controller.AdminLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>adminLoginController</servlet-name>
  	<url-pattern>/adminLoginController</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>AuthorityLogin</servlet-name>
  	<servlet-class>prof.controller.AuthorityLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AuthorityLogin</servlet-name>
  	<url-pattern>/AuthorityLoginController</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>VoteReviewAuthority</servlet-name>
  	<servlet-class>prof.controller.VoteReviewController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>VoteReviewAuthority</servlet-name>
  	<url-pattern>/voteListAuth</url-pattern>
  </servlet-mapping>
</web-app>